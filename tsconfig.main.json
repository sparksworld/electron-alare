{
  "compilerOptions": {
    "target": "esNext",
    "module": "commonjs",
    "outDir": ".build/main",
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {},
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules", ".build", "release"],
  "include": ["src/main/**/*.ts"]
}
