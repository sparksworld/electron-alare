{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "electron-build",
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "baseUrl": "electron",
    "paths": {},
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "node_modules",
    "build",
    "electron-build",
    "release"
  ],
  "include": [
    "electron/**/*.ts"
  ]
}
